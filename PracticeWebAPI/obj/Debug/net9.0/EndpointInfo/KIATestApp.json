  "components": {
    "schemas": {
      "Employee": {
        "required": [
          "designation",
          "dob",
          "fullName",
          "gender",
          "salary"
        ],
        "type": "object",
        "properties": {
          "id": {
            "type": "integer",
            "format": "int32"
          },
          "fullName": {
            "maxLength": 100,
            "minLength": 0,
            "type": "string"
          },
          "dob": {
            "minLength": 1,
            "type": "string"
          },
          "gender": {
            "minLength": 1,
            "type": "string"
          },
          "salary": {
            "type": "number",
            "format": "double"
          },
          "designation": {
            "minLength": 1,
            "type": "string"
          },
          "importedDate": {
            "type": "string",
            "format": "date-time"
          },
          "importedBy": {
            "type": "string",
            "nullable": true
          },
          "file": {
            "type": "string",
            "nullable": true
          },
          "fileSource": {
            "type": "string",
            "nullable": true
          },
          "photo": {
            "type": "string",
            "format": "byte",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "LoginModel": {
        "type": "object",
        "properties": {
          "username": {
            "type": "string",
            "nullable": true
          },
          "password": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      },
      "WeatherForecast": {
        "type": "object",
        "properties": {
          "date": {
            "type": "string",
            "format": "date-time"
          },
          "temperatureC": {
            "type": "integer",
            "format": "int32"
          },
          "temperatureF": {
            "type": "integer",
            "format": "int32",
            "readOnly": true
          },
          "summary": {
            "type": "string",
            "nullable": true
          }
        },
        "additionalProperties": false
      }
    }
